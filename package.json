{
  "name": "sgu",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prisma:generate": "prisma generate --config prisma-users.config.ts && prisma generate --config prisma-academic.config.ts && prisma generate --config prisma-profiles.config.ts",
    "migrate:dev:users": "prisma migrate dev --config prisma-users.config.ts",
    "migrate:dev:academic": "prisma migrate dev --config prisma-academic.config.ts",
    "migrate:dev:profiles": "prisma migrate dev --config prisma-profiles.config.ts",
    "migrate:dev:all": "npm run migrate:dev:users && npm run migrate:dev:academic && npm run migrate:dev:profiles",
    "migrate:deploy:users": "prisma migrate deploy --config prisma-users.config.ts",
    "migrate:deploy:academic": "prisma migrate deploy --config prisma-academic.config.ts",
    "migrate:deploy:profiles": "prisma migrate deploy --config prisma-profiles.config.ts",
    "migrate:deploy:all": "npm run migrate:deploy:users && npm run migrate:deploy:academic && npm run migrate:deploy:profiles",
    "migrate:status:users": "prisma migrate status --config prisma-users.config.ts",
    "migrate:status:academic": "prisma migrate status --config prisma-academic.config.ts",
    "migrate:status:profiles": "prisma migrate status --config prisma-profiles.config.ts",
    "prisma:studio:users": "prisma studio --config prisma-users.config.ts",
    "prisma:studio:academic": "prisma studio --config prisma-academic.config.ts",
    "prisma:studio:profiles": "prisma studio --config prisma-profiles.config.ts",
    "db:push:users": "prisma db push --config prisma-users.config.ts",
    "db:push:academic": "prisma db push --config prisma-academic.config.ts",
    "db:push:profiles": "prisma db push --config prisma-profiles.config.ts",
    "db:push:all": "npm run db:push:users && npm run db:push:academic && npm run db:push:profiles",
    "seed:users": "ts-node prisma/seed-users.ts",
    "seed:academic": "ts-node prisma/seed-academic.ts",
    "seed:profiles": "ts-node prisma/seed-profiles.ts",

    "seed:complete": "ts-node prisma/seeds/seed-complete-test-data.ts",
    "seed:all": "npm run seed:users && npm run seed:academic && npm run seed:profiles"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "dependencies": {
    "@nestjs/common": "^11.1.9",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.9",
    "@nestjs/jwt": "^11.0.2",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.9",
    "@nestjs/swagger": "^11.2.3",
    "@prisma/adapter-pg": "^7.1.0",
    "@prisma/client": "^7.1.0",
    "@prisma/internals": "^7.0.1",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "dotenv": "^17.2.3",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.16.3",
    "prisma": "^7.1.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "swagger": "^0.7.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.19.2",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^30.0.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
